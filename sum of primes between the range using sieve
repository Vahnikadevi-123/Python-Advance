## input ---> 1 6
   output---> 10



n=1000000
sieve=[1 for i in range(n+1)]
sieve[0]=0
sieve[1]=0
i=2
while(i*i<=n):
    if sieve[i]==1:
        for j in range(i*i,n+1,i):
            sieve[j]=0
    i+=1
a=int(input())
for k in range(a):
    l,r=map(int,input().split())
    sum1=0
    for m in range(l,r+1):
        if sieve[m]==1:
            sum1+=m
    print(sum1)
            
